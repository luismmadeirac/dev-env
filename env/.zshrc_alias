alias sz='source ~/.zshrc'

alias c='clear'


# Services
alias d='docker'
alias k='kubectl'
alias h='helm'
alias tf='terraform'

alias t='tmux'
alias v='nvim'

# Jump to places
alias p='cd $HOME/personal'
alias w='cd $HOME/work'
alias vaults='cd $HOME/vaults'
alias dev-env='cd $HOME/developer/dev-env'
alias devr='$HOME/.local/scripts/dev-env.sh'

# Shortcut: deploy and reload shell in one command
alias devrs='$HOME/.local/scripts/dev-env.sh --auto-approve && source ~/.zshrc'
